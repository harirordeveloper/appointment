<script type="text/javascript">
$(document).ready(function(){
    $('#example').dataTable();
});
</script>

<table>
	<tr>
		<td width="150px" style="font-family :arial;"><%= image_tag("add.png")%><font size="4" ><%= link_to 'Add Profile', new_clinic_profile_master_path %></font></td>
		<td width="850px"> </td><td><a href='<%= loginpage_path %>' target=_parent><%= image_tag("/assets/back.png",:border => 0)%></a></td>
	</tr>
</table>

<table id='example'>
  <thead>
    <th>Clinic</th>
    <th>Services</th>
    <th></th>
    
 </thead>
<tbody>
<% @clinic_profile_masters.each do |clinic_profile_master| %>
<tr>
    <td><%= clinic_profile_master.clinic.name %></td>
    <td><%= clinic_profile_master.services %></td>
   
    <td align="center"><%=  link_to image_tag("/assets/show.png",:border=>"0",:title=>" Click Here to VIEW This Record",:width=>20,:height=>20), clinic_profile_master %>&nbsp&nbsp&nbsp
    <%=  link_to image_tag("/assets/Edit.png",:border=>"0",:title=>" Click Here to Edit This Record",:width=>20,:height=>20), edit_clinic_profile_master_path(clinic_profile_master) %>&nbsp&nbsp&nbsp
   <%= link_to image_tag("/assets/delete.png",:border=>"0",:title=>" Click Here to Delete This Record",:width=>20,:height=>20), clinic_profile_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
 
<% end %>
</tr>
</tbody>
</table>
